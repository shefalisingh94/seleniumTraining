package Selenium;

public class Pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        //int i,j;
        int row=0;
        for(int i=0; i<row; i++) {
        	for (int j = 0;j<=i;j++) {
        	System.out.println("*");
        }
        	System.out.println();   
	}

}
